{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": [
		"pnpm-lock.yaml",
		"**.svg",
		".git/",
		".gitignore",
		".vscode/",
		"./patches/",
		"./packages/database/drizzle/"
	],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"buildinfo",
		"codeowner",
		"compat",
		"focusable",
		"isbot",
		"libsql",
		"linebreak",
		"pawelblaszczyk",
		"pipeable",
		"plusplus",
		"replicache",
		"taze",
		"textareas",
		"todos",
		"tsbuildinfo",
		"tseslint",
		"turso",
		"typecheck",
		"typedefs",
		"unitless"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"]
}
