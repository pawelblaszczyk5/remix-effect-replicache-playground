{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "./patches/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"buildinfo",
		"codeowner",
		"compat",
		"focusable",
		"isbot",
		"linebreak",
		"pawelblaszczyk",
		"pipeable",
		"plusplus",
		"taze",
		"textareas",
		"todofall",
		"tsbuildinfo",
		"tseslint",
		"typecheck",
		"typedefs",
		"unitless"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"]
}
